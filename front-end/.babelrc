{
  "presets": ["@babel/react", "@babel/env", "@babel/typescript"],
  "plugins": [
    ["import", {
      "libraryName": "antd",
      "libraryDirectory": "es",
      "style": "css" // `style: true` 会加载 less 文件
    }],
    [
      "@babel/plugin-transform-runtime", {
        "legacy": true
      }
    ]
  ]
}